anos = {}
lista = []
for c in range(1, 1001):    
        ano = int(input('Digite o ano: '))
        anos["ano"] = ano
        lista.append(ano)
def remove(lista):
    l = []
    for i in lista:
        if i not in l:
            l.append(i)
    l.sort()
    return l
listas = remove(lista)
for c in listas:
    n = lista.count(c)
    print(f'{c}: {n}')



